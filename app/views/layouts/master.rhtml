	<%= javascript_include_tag 'jquery_tweet'%>
        <%= javascript_include_tag 'jquery.countdown'%>

	<script type='text/javascript'> 
	$(document).ready(function(){
		$(".tweet").tweet({
			username: "ignitechennai",
			//join_text: "auto",
			//avatar_size: 73,
			count: 2,
			query: "from:ignitechennai",
			loading_text: "searching twitter..."

		});
		$('#one_line_abstract').hide();
		$("#registration_type_of_attendee").change(function() 
		    { 
		        var message_index 

		        message_index = $("#registration_type_of_attendee").val(); 
		        if (message_index == "Presenter") 
		            $("#one_line_abstract").show();
				else
					$("#one_line_abstract").hide();
		    });
                $('#byMonth').countdown({until: ( new Date() + 500), format: 'yowdHMS'});
                

	});
	</script>
	<%= stylesheet_link_tag 'style' %>
        <%= stylesheet_link_tag 'jquery.countdown' %>
